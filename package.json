{
  "name": "ember-steps",
  "version": "10.0.1",
  "description": "Ember addon for declaratively creating wizards, tabbed UIs, and more",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "release": "standard-version",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "main": "index.js",
  "homepage": "https://alexlafroscia.github.io/ember-steps",
  "engines": {
    "node": "10.* || >= 12"
  },
  "dependencies": {
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "tracked-maps-and-sets": "^3.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.2",
    "ember-cli": "~4.4.0",
    "ember-cli-addon-docs": "^5.0.0",
    "ember-cli-addon-docs-esdoc": "^0.4.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-document-title-northm": "^1.0.3",
    "ember-cli-fastboot": "^3.3.2",
    "ember-cli-fastboot-testing": "^0.6.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^2.2.1",
    "ember-concurrency-decorators": "^2.0.3",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-qunit-assert-helpers": "^0.2.2",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.5.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.10.1",
    "ember-truth-helpers": "^3.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.0.3",
    "loader.js": "^4.7.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "qunit-wait-for": "^2.0.2",
    "standard-version": "^9.5.0",
    "testdouble": "^3.16.6",
    "testdouble-qunit": "^3.0.3"
  },
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/alexlafroscia/ember-steps",
  "license": "MIT",
  "author": "Alex LaFroscia <alex@lafroscia.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://alexlafroscia.github.io/ember-steps"
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{md,json,yml}": "prettier --write"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  },
  "volta": {
    "node": "12.16.2",
    "yarn": "1.22.4"
  }
}
